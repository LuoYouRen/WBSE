<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
		 version="3.0"
		 metadata-complete="true">
  <display-name>FallingDown</display-name>
	<servlet>
		<servlet-name>FallingServlet</servlet-name>
		<servlet-class>ntou.cs.WBSE.FallingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FallingServlet</servlet-name>
		<url-pattern>/FallingServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>FallingRank</servlet-name>
		<servlet-class>ntou.cs.WBSE.FallingRank</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FallingRank</servlet-name>
		<url-pattern>/FallingRank</url-pattern>
	</servlet-mapping>	
	<welcome-file-list>
        <welcome-file>falling.html</welcome-file>
    </welcome-file-list>
</web-app>